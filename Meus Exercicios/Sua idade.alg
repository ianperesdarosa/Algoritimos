algoritmo "Sua idade"
var
     Passado: Inteiro
     presente: Inteiro
     idade: Inteiro
inicio
      Escreva ("Em que anos estamos: ")
      Leia (Presente)
      Escreva ("Que ano voce nasceu? ")
      Leia (Passado)
      idade <- (passado - presente)
      Escreval ("Voce tem ", idade, " Anos!")

fimalgoritmo
